generated":{"line":36,"column":0}}],"sources":{"src/button.vue":"<template>\r\n   <!-- 冒号是v-bind的缩写，··字符串模板\r\n    ${icon}为占位符\r\n    插槽slot\r\n    对于iconPosition: 有三种情况： ‘undefined‘：true， ‘left’：true；’right‘：true’\r\n    -->\r\n   <button class = \"g-button\" :class=\"{[`icon-${iconPosition}`]:true}\">\r\n    <g-icon v-if=\"icon\" :name = 'icon'></g-icon>\r\n    <div class = 'content'>\r\n      <slot></slot>\r\n    </div>\r\n  </button>\r\n\r\n</template>\r\n<script>\r\nexport default {\r\n  //设置接受的参数\r\n//   props: [\"icon\", \"iconPosition\"],避免undefined的出现\r\n  props: {\r\n      icon:{},\r\n      iconPosition:{\r\n        type: String,\r\n        default: \"left\",\r\n        // 属性检查器：validator\r\n        validator(value){\r\n          return (value=== \"left\" || value=== \"right\")\r\n        }\r\n        //   left 是字符串！\r\n      }\r\n  }\r\n}; \r\n</script>\r\n<style lang = \"scss\">\r\n.g-button {\r\n  font-size: var(--font-size);\r\n  height: var(--button-height);\r\n  padding: 0 0.5em; /*CSS技巧不写死宽度*/\r\n  font: inherit;\r\n  border-radius: var(--border-radius);\r\n  border: 1px solid var(--border-color);\r\n  background-color: var(--button-bg);\r\n  display: inline-flex; justify-content: center; align-items: center;\r\n  vertical-align: middle;\r\n  &:hover {border-color: var(--border-color-hover);}\r\n  &:active {background-color: var(--button-active-bg);}\r\n  &:focus {outline: none;}\r\n  > .content{order: 2;}\r\n  > .icon{order: 1; margin-right: .3em}\r\n  \r\n  &.icon-right{\r\n    > .content{order: 1;}\r\n    > .icon{order: 2; margin-right: 0em; margin-left: .3em}\r\n  }\r\n}\r\n</style>\r\n"},"lineCount":null},"css":"@charset \"UTF-8\";\n.g-button {\n  font-size: var(--font-size);\n  height: var(--button-height);\n  padding: 0 0.5em;\n  /*CSS技巧不写死宽度*/\n  font: inherit;\n  border-radius: var(--border-radius);\n  border: 1px solid var(--border-color);\n  background-color: var(--button-bg);\n  display: inline-flex;\n  justify-content: center;\n  align-items: center;\n  vertical-align: middle; }\n  .g-button:hover {\n    border-color: var(--border-color-hover); }\n  .g-button:active {\n    background-color: var(--button-active-bg); }\n  .g-button:focus {\n    outline: none; }\n  .g-button > .content {\n    order: 2; }\n  .g-button > .icon {\n    order: 1;\n    margin-right: .3em; }\n  .g-button.icon-right > .content {\n    order: 1; }\n  .g-button.icon-right > .icon {\n    order: 2;\n    margin-right: 0em;\n    margin-left: .3em; }\n"},"hash":"823e24a8fad4011cb3a80c59afb66e5c","cacheData":{"env":{}}}